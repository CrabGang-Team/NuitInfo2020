		{# Base CSS #}
		<link rel="stylesheet" href="{{ asset('css/base.css') }}">

        {# Emergency CSS #}
		<link rel="stylesheet" href="{{ asset('css/emergency.css') }}">

		{# Personal CSS #}
		<link rel="stylesheet" href="{{ asset('css/style.css') }}">
		<link rel="stylesheet" href="{{ asset('css/responsive.css') }}">
		
		{% block stylesheets %}{% endblock %}
	</head>
    <body style="background-color:hsla(147, 8%, 2%,0.5);">
    <div style="width:100vw; heigth:100vh !important;position:relative">
        <div class="meeting">
            <img src="{{ asset('images/among.png') }}" style="width:100%;heigth:100%; object-fit:cover" />
            <button id="div_Ant" onClick="emergencyDiv.ShowAnt()" style="position: absolute;background-color:transparent;heigth:10%; width:7%;left:5%;top:32.5%">
                <img src="{{ asset('images/Yellow.png') }}" style="width:100%;heigth:100%; object-fit:cover"/>
            </button>

            <button id="div_Alp" onClick="emergencyDiv.ShowAlf()" style="position: absolute;background-color:red;heigth:10%; width:7%;left:48.3%;top:32.5%">
            <img src="{{ asset('images/Blue.png') }}" style="width:100%;heigth:100%; object-fit:cover"/>
            </div>

            <button id="div_Rob" onClick="emergencyDiv.ShowRob()" style="position: absolute;background-color:red;heigth:10%; width:3.8%;left:49%;top:44%">
            <img src="{{ asset('images/Lime.png') }}" style="width:100%;heigth:100%; object-fit:cover"/>
            </div>
            
            <button id="button_Ant" type=submit style="display:none;width:3%;position: absolute;z-index:5000;left:41%;top:36.8%">
                <img src="{{ asset('images/voteYES.png') }}" style="width:100%;heigth:100%; object-fit:cover" onclick="emergencyDiv.antoinedied()"/>
            </button>
            <button id="button_Alf" type=submit style="display:none; width:3%;position: absolute;z-index:5000;left:62%;top:36.8%">
                <img src="{{ asset('images/voteYES.png') }}" style="width:100%;heigth:100%; object-fit:cover" onclick="emergencyDiv.alfreddied()"/>
            </button>
            <button id="button_Rob" type=submit style="display:none; width:3%;position: absolute;z-index:5000;left:62%;top:28.3%">
                <img src="{{ asset('images/voteYES.png') }}" style="width:100%;heigth:100%; object-fit:cover" onclick="emergencyDiv.robindied()"/>
            </button>
            <button id="button_AntNO" onClick="emergencyDiv.StopAnt()" style="display:none;width:3%;position: absolute;z-index:5000;left:44.2%;top:36.9%">
                <img src="{{ asset('images/voteNO.png') }}" style="width:100%;heigth:100%; object-fit:cover"/>
            </button>
            <button id="button_AlfNO" onClick="emergencyDiv.StopAlf()" style="display:none; width:3%;position: absolute;z-index:5000;left:65.2%;top:36.9%">
                <img src="{{ asset('images/voteNO.png') }}" style="width:100%;heigth:100%; object-fit:cover"/>
            </button>
            <button id="button_RobNO" onClick="emergencyDiv.StopRob()" style="display:none; width:3%;position: absolute;z-index:5000;left:65.2%;top:28.4%">
                <img src="{{ asset('images/voteNO.png') }}" style="width:100%;heigth:100%; object-fit:cover"/>
            </button>
        </div>
    </div>
   


    </body>
    
